{
  "name": "@aztec-otc-desk/contracts",
  "devDependencies": {
      "@types/bun": "catalog:",
      "@types/jest": "catalog:",
      "@aztec/aztec.js": "catalog:",
      "@aztec/accounts": "catalog:",
      "@aztec/ethereum": "catalog:",
      "@aztec/entrypoints": "catalog:",
      "@aztec/stdlib": "catalog:",
      "@aztec/noir-contracts.js": "catalog:",
      "jest": "catalog:"
  },
  "main": "./ts/src/index.ts",
  "types": "./ts/src/index.ts",
  "exports": {
    ".": "./ts/src/index.ts"
  },
  "peerDependencies": {
    "typescript": "catalog:"
  },
  "scripts": {
    "test": "bun run test:nr && bun test",
    "test:nr": "LOG_LEVEL=debug aztec test",
    "deploy-token": "bun run ./scripts/deploy_token.ts",
    "compile": "aztec-nargo compile",
    "codegen": "aztec codegen target --outdir artifacts",
    "copy-artifacts": "bun run scripts/add_artifacts.ts",
    "build": "bun run compile && bun run codegen -f && bun run copy-artifacts",
    "sandbox": "aztec start --sandbox",
    "pxe:local": "LOG_LEVEL=info aztec start --pxe --pxe.nodeUrl http://localhost:8080 --pxe.proverEnabled false",
    "pxe:local:1": "bun run pxe:local --port 8081 --p2p.p2pPort 40401",
    "pxe:testnet": "aztec start --pxe --pxe.nodeUrl=https://aztec-alpha-testnet-fullnode.zkv.xyz --l1-rpc-urls=https://eth-sepolia.public.blastapi.io --l1-chain-id 11155111",
    "pxe:testnet:1": "bun run pxe:testnet --port 8081 --p2p.p2pPort 40401"
  }
}